<%= form_for(subscribe) do |form| %>
  <div class="form-group icon-my">
  <i class="fa fa-address-book"></i>
    <%= form.text_field :name, class:"form-control", placeholder: "Digite seu nome" %>
  </div>
  <div class="form-group icon-my">
    <i class="fa fa-envelope-open"></i>
      <%= form.email_field :email, class:"form-control", placeholder:"Digite seu e-mail" %>
  </div>
  <%= form.submit "Inscreva-se", class:"btn btn-primay email-button" %>
<% end %>